<div class="container">
  <p id="notice"><%= notice %></p>

  <div id="page_title"><h1>Directory of Users</h1></div>

  <table>
    <thead>
      <tr>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td>
          <%= link_to user.name, user %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>

</div>
